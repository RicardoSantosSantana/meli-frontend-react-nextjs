{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts", 
    "**/*.ts", 
    "**/*.tsx", 
    "**/pages/*.ts",
    "**/pages/*.tsx",
    "**/pages/*.jsx"
  , "**/components/*.jsx", "src/components/product/New.jsx", "src/components/product/FormNewProduct.tsx"     ],
  "exclude": ["node_modules"]
}
